<script>
var xValues = ["COD Count","ONLINECount"];
var yValues = [{{codCount}}, {{ONLINECount}}];
var barColors = [
  "#b91d47",
  "#00aba9",
  "#2b5797",
  "#e8c3b9",
  "#1e7145"
];

new Chart("myChart", {
  type: "pie",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    title: {
      display: true,
      text: "Payment Details"
    }
  }
});
</script>

